<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.superAgentUpgradeReviewController">
   <children>
      <Button fx:id="closeButton" layoutX="480.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#closeWindow"
              text="Close">
         <font>
            <Font size="16.0"/>
         </font>
      </Button>
      <Button fx:id="approveButton" layoutX="161.0" layoutY="325.0" mnemonicParsing="false"
              onMouseClicked="#approveUpgradeRequest" text="Approve">
         <font>
            <Font size="16.0"/>
         </font>
      </Button>
      <Button fx:id="denyButton" layoutX="281.0" layoutY="325.0" mnemonicParsing="false"
              onMouseClicked="#denyUpgradeRequest" text="Deny">
         <font>
            <Font size="16.0"/>
         </font>
      </Button>
      <Label layoutX="58.0" layoutY="99.0" text="Full Name:"/>
      <Label layoutX="58.0" layoutY="158.0" text="User ID:"/>
      <Label layoutX="58.0" layoutY="188.0" text="Email:"/>
      <Label layoutX="58.0" layoutY="220.0" text="Phone Number:"/>
      <Label layoutX="58.0" layoutY="250.0" text="Current Authentication Level:"/>
      <Label layoutX="58.0" layoutY="280.0" text="Requested Authentication Level:"/>
      <Label layoutX="58.0" layoutY="128.0" text="Username:"/>
      <Label fx:id="nameLabel" alignment="CENTER_RIGHT" layoutX="331.0" layoutY="99.0" text="Label"/>
      <Label fx:id="usernameLabel" layoutX="331.0" layoutY="128.0" text="Label"/>
      <Label fx:id="uIDLabel" layoutX="332.0" layoutY="158.0" text="Label"/>
      <Label fx:id="emailLabel" layoutX="332.0" layoutY="188.0" text="Label"/>
      <Label fx:id="phoneNumberLabel" layoutX="332.0" layoutY="220.0" text="Label"/>
      <Label fx:id="currentAuthLabel" layoutX="332.0" layoutY="250.0" text="Label"/>
      <Label fx:id="requestedAuthLabel" layoutX="332.0" layoutY="280.0" text="Label"/>
      <Label layoutX="162.0" layoutY="24.0" styleClass="normalTitle" text="Upgrade Request">
         <font>
            <Font size="36.0"/>
         </font>
      </Label>
   </children>
</AnchorPane>

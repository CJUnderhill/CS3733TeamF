                    <PropertyValueFactory property="status"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
      <Button fx:id="backButton" layoutX="22.0" layoutY="23.0" mnemonicParsing="false" onMouseClicked="#returnToMainPage" text="Back" AnchorPane.leftAnchor="22.0" AnchorPane.topAnchor="23.0">

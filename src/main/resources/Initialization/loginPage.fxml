<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserAccounts.AuthenticationController">
   <children>
       <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#FAFFFF" height="500.0" layoutX="-25.0" layoutY="67.0"
                  stroke="BLACK" strokeType="INSIDE" width="1000.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.topAnchor="0.0"/>
       <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#597B99" height="100.0" layoutX="667.0" layoutY="664.0"
                  stroke="BLACK" strokeType="INSIDE" width="1000.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.topAnchor="0.0"/>
      <TextField fx:id="username" layoutX="351.0" layoutY="133.0" promptText="Username">
         <font>
            <Font size="24.0" />
         </font></TextField>
       <TextField fx:id="password_text" layoutX="351.0" layoutY="199.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font></TextField>
      <Button fx:id="loginButton" layoutX="351.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#loginAction" text="Log In">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Label fx:id="errorLabel" layoutX="341.0" layoutY="259.0" prefHeight="17.0" prefWidth="318.0">
         <font>
            <Font size="18.0" />
         </font></Label>
       <Button fx:id="create_user_button" layoutX="500.0" layoutY="298.0" mnemonicParsing="false"
               onMouseClicked="#displayCreateUserPage" text="Create User">
         <font>
            <Font size="24.0" />
         </font></Button>
       <Button fx:id="returnButton" layoutX="30.0" layoutY="30.0" mnemonicParsing="false" onMouseClicked="#returnToMain"
               text="Return To Main">
         <font>
            <Font size="18.0" />
         </font></Button>
       <Label layoutX="415.0" layoutY="30.0" text="User Login">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UserAccounts.AuthenticationController">
   <children>
      <TextField fx:id="username" layoutX="351.0" layoutY="133.0" promptText="Username">
         <font>
            <Font size="24.0" />
         </font></TextField>
      <TextField fx:id="password" layoutX="351.0" layoutY="199.0" promptText="Password">
         <font>
            <Font size="24.0" />
         </font></TextField>
      <Button fx:id="loginButton" layoutX="351.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#loginAction" text="Log In">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Label fx:id="errorLabel" layoutX="341.0" layoutY="259.0" prefHeight="17.0" prefWidth="318.0">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button layoutX="500.0" layoutY="298.0" mnemonicParsing="false" onMouseClicked="#displayCreateUserPage" text="Create User">
         <font>
            <Font size="24.0" />
         </font></Button>
      <Button fx:id="returnButton" layoutX="27.0" layoutY="32.0" mnemonicParsing="false" onMouseClicked="#returnToMain" text="Return To Main">
         <font>
            <Font size="18.0" />
         </font></Button>
      <Label layoutX="415.0" layoutY="52.0" text="User Login">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
